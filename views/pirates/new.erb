<h1>Make your form here</h1>

<form action="/pirates" method="post">
  <h2>Pirate</h2>
  <label for="pirate[name]">Name</label>
  <input id="pirate_name" type="text" name="pirate[name]" >

  <label for="pirate[weight]">Weight</label>
  <input id="pirate_weight" type="text" name="pirate[weight]" >

  <label for="pirate[height]">Height</label>
  <input id="pirate_height" type="text" name="pirate[height]" >

  <h2>Ships</h2>
  <h3>Ship One</h3>
  <label for="pirate[ships][][name]">Name</label>
  <input id="ship_name_1" type="text" name="pirate[ships][][name]">
  <label for="pirate[ships][][type]">Type</label>
  <input id="ship_type_1" type="text" name="pirate[ships][][type]">
  <label for="pirate[ships][][booty]">Booty</label>
  <input id="ship_booty_1" type="text" name="pirate[ships][][booty]">

  <h3>Ship Two</h3>
  <label for="pirate[ships][][name]">Name</label>
  <input id="ship_name_2" type="text" name="pirate[ships][][name]">
  <label for="pirate[ships][][type]">Type</label>
  <input id="ship_type_2" type="text" name="pirate[ships][][type]">
  <label for="pirate[ships][][booty]">Booty</label>
  <input id="ship_booty_2" type="text" name="pirate[ships][][booty]">

  <input id="Submit" type="submit" value="submit"/>
</form>
